{
  "algorithms": {
    "dqn_full": {
      "components": {
        "action_mask": true,
        "constraint": true,
        "reward_shaping": true,
        "safety": true
      },
      "type": "dqn"
    },
    "dqn_vanilla": {
      "components": {
        "action_mask": false,
        "constraint": false,
        "reward_shaping": false,
        "safety": false
      },
      "type": "dqn"
    },
    "greedy": {
      "class": "GreedyAgent",
      "type": "heuristic"
    },
    "random": {
      "class": "RandomAgent",
      "type": "baseline"
    }
  },
  "environments": [
    "config_5.json"
  ],
  "training": {
    "batch_size": 64,
    "episodes": 100,
    "epsilon_decay": 0.995,
    "epsilon_end": 0.02,
    "epsilon_start": 1.0,
    "gamma": 0.99,
    "learning_rate": 0.001,
    "memory_capacity": 10000,
    "target_update_freq": 100
  },
  "evaluation": {
    "episodes": 50,
    "final_eval_episodes": 10,
    "metrics": [
      "cumulative_reward",
      "success_rate",
      "episode_length",
      "training_time",
      "constraint_violations"
    ],
    "seeds": [
      42,
      123,
      456
    ]
  },
  "output": {
    "generate_plots": true,
    "results_dir": "unified_comparison_results",
    "save_models": true,
    "statistical_tests": true
  }
}